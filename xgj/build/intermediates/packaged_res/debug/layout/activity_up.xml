                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textSize="@dimen/txt_size_default" />
            <EditText
                android:id="@+id/editText1"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:ems="10"
